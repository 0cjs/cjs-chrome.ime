{
  "name": "cjs IME",
  "version": "0.1",
  "manifest_version": 2,
  "description": "XXX",
  "background":  {
    "scripts": ["main.js"]
  },
  "permissions": [
    "input"
  ],
  "input_components": [
    {
      "name": "cjs IME",
      "type": "ime",
      "id": "cjs",
      "description": "cjs IME",  // A user visible description
      "language": "en-US",  // The primary language this IME is used for
      "layouts": ["us::eng"]  // The supported keyboard layouts for this IME
    }
  ]
}
